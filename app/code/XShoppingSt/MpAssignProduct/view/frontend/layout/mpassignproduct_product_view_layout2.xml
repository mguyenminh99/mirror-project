<?xml version="1.0"?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" layout="seller-2columns-left" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <update handle="marketplace_styles"/>
    <head>
        <css src="XShoppingSt_MpAssignProduct::css/styles.css"/>
    </head>
    <body>
        <referenceBlock name="seller.page.main.title">
            <action method="setPageTitle">
                <argument translate="true" name="title" xsi:type="string">Marketplace Assign Product</argument>
            </action>
        </referenceBlock>
        <referenceContainer name="seller.content">
            <block class="XShoppingSt\MpAssignProduct\Block\Product\ProductList" name="assignproduct_view" template="XShoppingSt_MpAssignProduct::view.phtml" cacheable="false"></block>
        </referenceContainer>
    </body>
</page>
