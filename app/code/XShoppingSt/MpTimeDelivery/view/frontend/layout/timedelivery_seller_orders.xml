<?xml version="1.0"?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <update handle="customer_account"/>
    <head>
        <css src="XShoppingSt_MpTimeDelivery::css/style.css"/>
        <css src="XShoppingSt_Marketplace::css/product.css"/>
    </head>
    <body>
        <referenceContainer name="content">
            <block class="XShoppingSt\MpTimeDelivery\Block\Orders"  name="manage-time-delivery-orders" template="seller/orders.phtml" cacheable="false">
            	<arguments>
	                <argument name="jsLayout" xsi:type="array">
	                    <item name="components" xsi:type="array">
	                        <item name="delivery-order-grid" xsi:type="array">
	                            <item name="component" xsi:type="string">XShoppingSt_MpTimeDelivery/js/view/grid</item>
	                        </item>
	                    </item>
	                </argument>
	            </arguments>
            </block>
        </referenceContainer>
    </body>
</page>
