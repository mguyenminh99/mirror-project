<?xml version="1.0"?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" layout="seller-2columns-left" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceBlock name="seller.page.main.title">
            <action method="setPageTitle">
                <argument translate="true" name="title" xsi:type="string">Transactions</argument>
            </action>
        </referenceBlock>
        <referenceContainer name="seller.content">
            <block class="XShoppingSt\Marketplace\Block\Transaction\Withdrawal" name="marketplace_transaction_withdrawal" template="transaction/withdrawal.phtml" cacheable="false"/>
            <uiComponent name="marketplace_transactions_listing"/>
        </referenceContainer>
    </body>
</page>
