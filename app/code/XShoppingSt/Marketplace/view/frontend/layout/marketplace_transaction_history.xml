<?xml version="1.0"?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <update handle="customer_account"/>
    <update handle="marketplace_styles"/>
    <body>
        <referenceBlock name="page.main.title">
            <action method="setPageTitle">
                <argument translate="true" name="title" xsi:type="string">Transactions</argument>
            </action>
        </referenceBlock>
        <referenceContainer name="content">
            <block class="XShoppingSt\Marketplace\Block\Transaction\Withdrawal" name="marketplace_transaction_withdrawal" template="transaction/withdrawal.phtml" cacheable="false"/>
            <block class="XShoppingSt\Marketplace\Block\Transaction\History" name="marketplace_transaction_history" template="transaction/history.phtml" cacheable="false"/>
        </referenceContainer>
    </body>
</page>
