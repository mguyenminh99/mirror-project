<?xml version="1.0"?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" layout="seller-2columns-left" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <head>
        <css src="XShoppingSt_MpMassUpload::css/styles.css"/>
    </head>
    <body>
        <referenceBlock name="seller.page.main.title">
            <action method="setPageTitle">
                <argument translate="true" name="title" xsi:type="string">Mass Upload Dataflow Profile</argument>
            </action>
        </referenceBlock>
        <referenceContainer name="seller.content">
            <block class="XShoppingSt\MpMassUpload\Block\Dataflow\Profile" name="mpmassupload_dataflow_profile" template="XShoppingSt_MpMassUpload::account/dataflow_profile.phtml" cacheable="false"></block>
        </referenceContainer>
    </body>
</page>
