<?xml version="1.0"?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" layout="seller-2columns-left" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <head>
        <css src="XShoppingSt_Marketplace::css/layout.css"/>
    </head>
    <body>
        <referenceBlock name="seller.page.main.title">
            <action method="setPageTitle">
                <argument translate="true" name="title" xsi:type="string">Create Sub Account</argument>
            </action>
        </referenceBlock>
        <referenceContainer name="seller.content">
            <block class="Magento\Framework\View\Element\Template" name="seller.create" template="Mpx_Marketplace::account/create_seller.phtml" cacheable="false"/>
        </referenceContainer>
    </body>
</page>
